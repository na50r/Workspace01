@Library('shared-library') _

pipeline {
    agent any
    stages {
        stage('Hello') {
            steps {
                helloWorldV1(name: 'Name', dayOfWeek: 'Day')
            }
        }
        stage('Run') {
            steps {
                dir('Workspace01/kubernetes/jobs') {
                    // Now you're in the 'Workspace01/kubernetes/jobs' directory
                    k8sDeployLocal('Job5')
                }
            }
        }
    }
}
